{
  "name": "babs",
  "type": "module",
  "version": "0.3.0",
  "engines": {
    "node": "v21"
  },
  "volta": {
    "node": "21.2.0"
  },
  "scripts": {
    "playerdev": "cross-env FE_ENV=playerdev vite --mode playerdev",
    "sharedln": "ln -f ./../proxima/src/shared/* ./src/shared",
    "sharedlnwin": "cmd /c if exist .\\src\\shared rmdir .\\src\\shared && mklink /D .\\src\\shared ..\\..\\proxima\\src\\shared",
    "dev": "vite --host",
    "pdevhttps": "cross-env FE_ENV=playerdev FE_HTTPS=true vite --mode playerdev",
    "commitlist": "git --no-pager log -8 --pretty=format:\"%h - %an, %ar : %s\"",
    "//check": "svelte-check --tsconfig ./tsconfig.json",
    "serve": "vite preview",
    "build": "vite build",
    "postbuild": "sh buildtime.sh",
    "predeploy": "npm run build",
    "deploy": "rsync --rsync-path 'sudo -u fe rsync' -azP --delete --exclude .gitignore ./dist/ root@earth.suncapped.com:/home/fe/expressa/public/babs",
    "// Disabling for now, too noisy! // postdeploy": "node discord.js",
    "log": "git --no-pager log --pretty=oneline --decorate=short -n10",
    "clean": "rm -rf node_modules/*",
    "cleanwin": "rmdir purgetemp 2>nul & mkdir purgetemp && robocopy /LOG:nul /MIR purgetemp node_modules && rmdir node_modules && timeout /T 1 && rmdir purgetemp && mkdir node_modules && npm run diwin",
    "diwin": "powershell -command \"Set-Content -Path './node_modules' -Stream com.dropbox.ignored -Value 1\""
  },
  "devDependencies": {
    "@suncapped/eslint-plugin": "file:eslint",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/dat.gui": "^0.7.12",
    "@types/js-cookie": "^3.0.6",
    "@types/luxon": "^3.3.7",
    "@types/node": "^20.10.5",
    "@types/suncalc": "^1.9.2",
    "@types/three": "^0.158.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "bicubic-interpolate": "^1.0.4",
    "cross-env": "^7.0.3",
    "deep-object-diff": "^1.1.9",
    "eslint": "^8.56.0",
    "lil-gui": "^0.19.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "three": "^0.158.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "webhook-discord": "^3.7.8"
  },
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "@neodrag/svelte": "^2.0.3",
    "dat.gui": "^0.7.9",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "keycode-js": "^3.1.0",
    "luxon": "^3.4.4",
    "suncalc": "^1.9.0",
    "troika-three-text": "^0.49.0"
  }
}
