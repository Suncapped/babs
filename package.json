{
  "name": "babs",
  "type": "module",
  "version": "0.3.0",
  "engines": {
    "node": "v20"
  },
  "volta": {
    "node": "20.2.0"
  },
  "scripts": {
    "playerdev": "cross-env FE_ENV=playerdev vite --mode playerdev",
    "predev": "ln -f ./../proxima/src/shared/* ./src/shared",
    "dev": "vite --host",
    "pdevhttps": "cross-env FE_ENV=playerdev FE_HTTPS=true vite --mode playerdev",
    "commitlist": "git --no-pager log -8 --pretty=format:\"%h - %an, %ar : %s\"",
    "//check": "svelte-check --tsconfig ./tsconfig.json",
    "serve": "vite preview",
    "build": "vite build",
    "postbuild": "sh buildtime.sh",
    "predeploy": "npm run build",
    "deploy": "rsync --rsync-path 'sudo -u fe rsync' -azP --delete --exclude .gitignore ./dist/ root@earth.suncapped.com:/home/fe/expressa/public/babs",
    "// Disabling for now, too noisy! // postdeploy": "node discord.js",
    "log": "git --no-pager log --pretty=oneline --decorate=short -n10"
  },
  "devDependencies": {
    "@suncapped/eslint-plugin": "file:eslint",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@tsconfig/svelte": "^5.0.2",
    "@types/dat.gui": "^0.7.10",
    "@types/js-cookie": "^3.0.3",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.6.0",
    "@types/suncalc": "^1.9.0",
    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "bicubic-interpolate": "^1.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "lil-gui": "^0.18.2",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "webhook-discord": "^3.7.8"
  },
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "@neodrag/svelte": "^2.0.3",
    "dat.gui": "^0.7.9",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "luxon": "^3.4.3",
    "suncalc": "^1.9.0",
    "three": "^0.156.1",
    "troika-three-text": "^0.48.0"
  }
}
